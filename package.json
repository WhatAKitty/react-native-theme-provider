    "theme"
  ],
  "author": "WhatAKitty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WhatAKitty/react-theme-provider/issues"
  },
  "homepage": "https://github.com/WhatAKitty/react-theme-provider#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "expect.js": "^0.3.1",
    "metro-react-native-babel-preset": "^0.49.0",
    "mocha": "^5.2.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": ">=16.5.0",
    "react-dom": ">=16.5.0",
    "react-native": ">=0.57.0"
